  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "babel-loader": "^9.1.2",
    "terser-webpack-plugin": "^5.3.8",
    "tslint": "^6.1.3",
    "tslint-jsdoc-rules": "^0.2.0",
